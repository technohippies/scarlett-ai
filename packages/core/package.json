{
  "name": "@scarlett/core",
  "version": "0.0.1",
  "description": "Core business logic and shared types for Scarlett",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./src/types/index.ts"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./src/services/index.ts"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./src/models/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3"
  }
}